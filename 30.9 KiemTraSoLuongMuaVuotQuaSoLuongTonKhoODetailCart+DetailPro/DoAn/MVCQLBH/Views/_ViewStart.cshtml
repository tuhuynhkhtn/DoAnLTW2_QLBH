@using MVCQLBH.Models
@using MVCQLBH.Ultilities

@if (Html.IsLoggedAdmin() == true && AddHelpers.ProductAdmin != true)
{
    Layout = "~/Views/Shared/_LayoutManageProduct.cshtml";
}
else if((Html.IsLoggedAdmin() == true) && AddHelpers.ProductAdmin == true)
{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
else
{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
